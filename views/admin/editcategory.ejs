<section id="main-content">
    <section class="wrapper">
        <h1>Edit Category</h1>
        <div class="container">
            <div class="row justify-content-center">
              <div class="col-md-6">
                <form action="/admin/category/update/<%= category._id %>" method="post">
                  <div class="form-group">
                    <label for="categoryName"><i class="fas fa-user"></i>Category</label>
                    <input type="text" name="category" class="form-control" value="<%= category.category %>" id="productName" placeholder="Enter product name" required>
                  </div>
                  <div class="form-group">
                    <label for="productDescription"><i class="fas fa-file-alt"></i> Description</label>
                    <textarea class="form-control" name="description" id="productDescription" required><%- category.description %></textarea>

                  </div>
                  
                  <button type="submit" class="btn btn-primary"><i class="fas fa-check"></i> Submit</button>
                </form>
              </div>
            </div>
          </div>
        
        







    </section>
</section>






