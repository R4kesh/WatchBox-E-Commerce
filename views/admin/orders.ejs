                        <% user.forEach((details,index) => { %>
                            

                           
                            <tr class='align-middle'>
                                <td style="color: black;"><%= index+1 %></td>
                                <td style="color: black;"><%= details._id %></td>
                                <%orderItems.forEach(items=>{%>

                               
