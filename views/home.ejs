	<div class="product">
		<div class="container">
			<div class="product-top">
				
				<div class="row">
					<% products.slice().reverse().forEach((row, index) => { %>
						<div class="col-md-3 product-left" style="margin-bottom: 20px;">
							<div class="product-main simpleCart_shelfItem">
								
								<a href="/productdetails/<%= row._id %>" class="mask">
									<img class="img-responsive zoom-img" src="/productimg/<%= row.image.length > 0 ? row.image[0] : 'default-image.jpg' %>" alt="<%= row.image %>" />
								</a>
								<div class="wishlist-icon">
									<a href="/addtowish/<%=row._id%>" class="btn btn-outline-danger"><i class="fa fa-heart"></i></a>
								 </div>
								
									<div class="row">
									<h3><%= row.name %></h3>
									<p>Explore Now</p>
									<%if(row.OfferPrice){%>
										<h4><span class="item_price">&#8377;<s><%= row.price %></s></span><span>  &#8377;<%= row.OfferPrice %></span></h4>
										<!-- <h4><span class="item_price">&#8377;</span></h4> -->
									<% }else{ %>
										<h4><span class="item_price">&#8377;<%= row.price %></span></h4>
										<% } %>	
										<h4>
											<a class="btn btn-secondary" href="/productdetails/<%= row._id %>">View Details</a>
										
											<% if (row.stock > 0) { %>
												<a class="btn btn-primary" href="/addtocart/<%= row._id %>">Add to Cart</a>
											<% } else { %>
												<button class="btn btn-primary" disabled>Out of Stock</button>
											<% } %>
										
											<!-- The price display -->
										</h4>
										
									
									
								</div>
								<%if(row.Discount>0){%>
								<div class="srch">
									<span>Save up to<%=row.Discount%>%</span>
								</div>
								<% } %>
							</div>
							
						</div>
					<% }) %>
				</div>
			</div>
		</div>
		<div class="container">
			<div class="row justify-content-center">
				
				<div class="col-md-12 text-center">
					<% if (currentPage > 1) { %>
						<a href="?page=<%= currentPage - 1 %>" class="btn btn-primary">Previous</a>
					<% } %>
				
					<!-- You might need to calculate if there are more products -->
					
    
					<a href="?page=<%= currentPage + 1 %>" class="btn btn-primary">Next</a>
					
				</div>
			</div>
